@model IEnumerable<YourProjectName.Models.Insuree>

<h2>Admin Dashboard - All Quotes</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Calculated Quote</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) {
            <tr>
                <td>@item.FirstName</td>
                <td>@item.LastName</td>
                <td>@item.EmailAddress</td>
                <td>@String.Format("{0:C}", item.Quote)</td>
            </tr>
        }
    </tbody>
</table>